<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jinhx.blog.mapper.sys.SysParamMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.jinhx.blog.entity.sys.SysParam">
        <id column="sys_param_id" property="sysParamId" />
        <result column="par_key" property="parKey" />
        <result column="par_value" property="parValue" />
        <result column="menu_url" property="menuUrl" />
        <result column="type" property="type" />
        <result column="creater_id" property="createrId" />
        <result column="updater_id" property="updaterId" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 查询所有结果列 -->
    <sql id="AllColumn">
        id, par_key, par_value, menu_url, type,creater_id,updater_id,create_time,update_time
    </sql>

</mapper>
